<li><%= question.content %>
  <ul>
    <% question.options.each do |option| %>
      <li><%= option.content %></li>
    <% end %>
  </ul>
</li>


<form method="POST" action="/questions/<%= question.id %>">
  <input type=hidden name="_method" value="DELETE">
  <input type="submit" value="Delete Question">
</form>


